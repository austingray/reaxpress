{
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "env": {
      "jquery": true,
      "browser": true
  },
  "rules": {
    "react/forbid-prop-types": "off",
    "no-console": "off"
  },
  "extends": "airbnb",
  "plugins": [
      "react",
      "jsx-a11y",
      "import"
  ]
}
