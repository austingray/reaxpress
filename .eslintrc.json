{
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "env": {
      "jquery": true,
      "browser": true
  },
  "rules": {
    "react/forbid-prop-types": "off",
    "no-console": "off",
    "react/prefer-stateless-function": ["off", { "ignorePureComponents": true }],
    "no-param-reassign": ["error", { "props": false }]
  },
  "extends": "airbnb",
  "plugins": [
      "react",
      "jsx-a11y",
      "import"
  ]
}
